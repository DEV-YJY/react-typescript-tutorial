{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "packageRules": [
    {
      "packagePatterns": ["*"],
      "enabled": false
    },
    {
      "matchDepNames": [
        "typescript",
        "vitest",
        "react",
        "@types/react",
        "@total-typescript/exercise-cli"
      ],
      "enabled": true,
      "automerge": true
    }
  ]
}
